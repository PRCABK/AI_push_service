<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.pushtool.mapper.SysAiConfigMapper">

    <resultMap type="SysAiConfig" id="SysAiConfigResult">
        <result property="id" column="id" />
        <result property="configName" column="config_name" />
        <result property="apiKey" column="api_key" />
        <result property="apiUrl" column="api_url" />
        <result property="modelName" column="model_name" />
        <result property="temperature" column="temperature" />
        <result property="maxTokens" column="max_tokens" />
        <result property="topP" column="top_p" />
        <result property="presencePenalty" column="presence_penalty" />
        <result property="frequencyPenalty" column="frequency_penalty" />
        <result property="remark" column="remark" />
    </resultMap>

    <sql id="selectSysAiConfigVo">
        select id, config_name, api_key, api_url, model_name, temperature, max_tokens, top_p, presence_penalty,
               frequency_penalty, remark
        from sys_ai_config
    </sql>

    <select id="selectSysAiConfigList" parameterType="SysAiConfig" resultMap="SysAiConfigResult">
        <include refid="selectSysAiConfigVo"/>
        <where>
            <if test="configName != null  and configName != ''"> and config_name like concat('%', #{configName}, '%')</if>
            <if test="apiKey != null  and apiKey != ''"> and api_key = #{apiKey}</if>
            <if test="apiUrl != null  and apiUrl != ''"> and api_url = #{apiUrl}</if>
            <if test="temperature != null "> and temperature = #{temperature}</if>
            <if test="modelName != null  and modelName != ''"> and model_name like concat('%', #{modelName}, '%')</if>
            <if test="maxTokens != null "> and max_tokens = #{maxTokens}</if>
        </where>
    </select>

    <insert id="insertSysAiConfig" parameterType="SysAiConfig" useGeneratedKeys="true" keyProperty="id">
        insert into sys_ai_config
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="configName != null and configName != ''">config_name,</if>
            <if test="apiKey != null and apiKey != ''">api_key,</if>
            <if test="apiUrl != null">api_url,</if>
            <if test="modelName != null">model_name,</if>
            <if test="temperature != null">temperature,</if>
            <if test="maxTokens != null">max_tokens,</if>
            <if test="topP != null">top_p,</if>
            <if test="presencePenalty != null">presence_penalty,</if>
            <if test="frequencyPenalty != null">frequency_penalty,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="configName != null and configName != ''">#{configName},</if>
            <if test="apiKey != null and apiKey != ''">#{apiKey},</if>
            <if test="apiUrl != null">#{apiUrl},</if>
            <if test="modelName != null">#{modelName},</if>
            <if test="temperature != null">#{temperature},</if>
            <if test="maxTokens != null">#{maxTokens},</if>
            <if test="topP != null">#{topP},</if>
            <if test="presencePenalty != null">#{presencePenalty},</if>
            <if test="frequencyPenalty != null">#{frequencyPenalty},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateSysAiConfig" parameterType="SysAiConfig">
        update sys_ai_config
        <trim prefix="SET" suffixOverrides=",">
            <if test="configName != null and configName != ''">config_name = #{configName},</if>
            <if test="apiKey != null and apiKey != ''">api_key = #{apiKey},</if>
            <if test="apiUrl != null">api_url = #{apiUrl},</if>
            <if test="modelName != null">model_name = #{modelName},</if>
            <if test="temperature != null">temperature = #{temperature},</if>
            <if test="maxTokens != null">max_tokens = #{maxTokens},</if>
            <if test="topP != null">top_p = #{topP},</if>
            <if test="presencePenalty != null">presence_penalty = #{presencePenalty},</if>
            <if test="frequencyPenalty != null">frequency_penalty = #{frequencyPenalty},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSysAiConfigById" parameterType="Long">
        delete from sys_ai_config where id = #{id}
    </delete>

    <delete id="deleteSysAiConfigByIds" parameterType="String">
        delete from sys_ai_config where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectSysAiConfigById" parameterType="Long" resultMap="SysAiConfigResult">
        select id, config_name, api_key, api_url, model_name, temperature, max_tokens, top_p,
               presence_penalty, frequency_penalty, remark
        from sys_ai_config where id = #{id}
    </select>

</mapper>
