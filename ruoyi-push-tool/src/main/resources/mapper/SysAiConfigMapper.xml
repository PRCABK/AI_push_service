<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.pushtool.mapper.SysAiConfigMapper">
    <resultMap type="SysAiConfig" id="SysAiConfigResult">
        <result property="id" column="id" />
        <result property="configName" column="config_name" />
        <result property="apiKey" column="api_key" />
        <result property="apiUrl" column="api_url" />
        <result property="modelName" column="model_name" />
        <result property="temperature" column="temperature" />
    </resultMap>

    <select id="selectSysAiConfigById" parameterType="Long" resultMap="SysAiConfigResult">
        select id, config_name, api_key, api_url, model_name, temperature from sys_ai_config where id = #{id}
    </select>
</mapper>