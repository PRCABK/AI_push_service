<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.pushtool.mapper.SysAiPromptMapper">

    <resultMap type="SysAiPrompt" id="SysAiPromptResult">
        <result property="id" column="id" />
        <result property="promptName" column="prompt_name" />
        <result property="promptKey" column="prompt_key" />
        <result property="content" column="content" />
    </resultMap>

    <select id="selectByPromptKey" parameterType="String" resultMap="SysAiPromptResult">
        select id, prompt_name, prompt_key, content
        from sys_ai_prompt
        where prompt_key = #{promptKey}
            limit 1
    </select>

</mapper>